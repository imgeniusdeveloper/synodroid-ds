<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent" android:orientation="vertical" android:padding="10dip">
 
<LinearLayout android:id="@+id/LinearLayout02" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content"><TextView android:id="@+id/TextView01" android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="@string/seeding_ratio" android:textAppearance="?android:attr/textAppearanceMedium"></TextView><EditText android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="100" android:id="@+id/seedingPercentage" android:inputType="number"></EditText><TextView android:id="@+id/TextView02" android:layout_height="wrap_content" android:text="@string/seeding_time" android:layout_width="fill_parent" android:layout_marginTop="10dip" android:textAppearance="?android:attr/textAppearanceMedium"></TextView><Spinner android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/seedingTime" android:entries="@array/seeding_time_array"></Spinner></LinearLayout>




<LinearLayout android:id="@+id/LinearLayout03" android:layout_height="wrap_content" android:layout_width="fill_parent" android:layout_marginTop="30dip" android:gravity="center_horizontal"><Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/button_ok" android:minWidth="120dip" android:id="@+id/OkBtn"></Button>
<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dip" android:minWidth="120dip" android:text="@string/button_cancel" android:id="@+id/CancelBtn"></Button>
</LinearLayout>
</LinearLayout>